{
  "name": "occode",
  "version": "0.2.6",
  "private": true,
  "description": "OCcode â€” branded cross-platform IDE wrapper with OpenClaw extension",
  "workspaces": [
    "apps/wrapper",
    "apps/extension",
    "apps/web",
    "packages/control-center"
  ],
  "scripts": {
    "dev": "npm run --workspace=apps/extension watch & npm run --workspace=apps/wrapper start",
    "web": "npm run --workspace=apps/web dev",
    "compile": "npm run ext:compile",
    "build-electron": "npm run --workspace=apps/wrapper build",
    "wrapper": "npm run --workspace=apps/wrapper start",
    "ext:compile": "npm run --workspace=apps/extension compile",
    "ext:watch": "npm run --workspace=apps/extension watch",
    "ext:package": "npm run --workspace=apps/extension package",
    "ext:bundle": "bash ./scripts/package-extension.sh",
    "vscodium:update": "node ./scripts/update-vscodium.mjs"
  },
  "dependencies": {
    "cobe": "^0.6.5",
    "framer-motion": "^12.34.2"
  }
}
